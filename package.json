{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "react服务器端渲染",
  "main": "src/server/index.js",
  "scripts": {
    "dev": "./node_modules/npm-run-all/bin/npm-run-all/index.js --parallel \"dev:server:**\" \"dev:client:**\"   ",
    "dev:server": "./node_modules/npm-run-all/bin/npm-run-all/index.js --parallel \"dev:server:**\"   ",
    "dev:server:run": "./node_modules/nodemon/bin/nodemon.js --watch ./src/server/dist/ --exec node ./src/server/dist/index.js ",
    "dev:server:build": "./node_modules/webpack/bin/webpack.js --config ./build/webpack.server.js --watch",
    "dev:client:build": "./node_modules/webpack/bin/webpack.js --config ./build/webpack.client.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wuyxp/react-ssr.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wuyxp/react-ssr/issues"
  },
  "homepage": "https://github.com/wuyxp/react-ssr#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "babel": "^6.23.0",
    "babel-loader": "^8.0.4",
    "babel-plugin-root-import": "^6.1.0",
    "babel-preset-react": "^6.24.1",
    "nodemon": "^1.18.6",
    "npm-run-all": "^4.1.3",
    "webpack": "^4.25.1",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  }
}
